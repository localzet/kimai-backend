{
  "name": "kimai-nest",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:prod": "node dist/main.js",
    "start:worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "test:integration": "ts-node ./scripts/integration-ml-test.ts",
    "build": "npm run openapi:export && tsc -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "test": "jest --runInBand",
    "openapi:export": "node -r ts-node/register src/scripts/export-openapi.ts",
    "e2e:up": "docker-compose -f docker-compose.test.yml up -d --build",
    "e2e:down": "docker-compose -f docker-compose.test.yml down",
    "test:e2e": "npm run e2e:up && jest --runInBand test/e2e.spec.ts && npm run e2e:down"
  },
  "dependencies": {
    "@bull-board/express": "^6.16.1",
    "@bull-board/nestjs": "^6.16.1",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.7.5",
    "@keyv/redis": "^5.1.5",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/client": "^5.0.0",
    "axios": "^1.5.0",
    "axios-retry": "^3.3.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "express": "^5.2.1",
    "ioredis": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "nestjs-zod": "^5.0.1",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "swagger-themes": "^1.4.3",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.21",
    "@types/node": "^18.19.130",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
